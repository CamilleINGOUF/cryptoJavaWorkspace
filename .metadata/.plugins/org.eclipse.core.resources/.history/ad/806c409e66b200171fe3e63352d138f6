		String key = findKey(text);
	}

	private static String findKey(String text)
	{
		
		return null;
	}
	
	
	
	private static boolean isUpper(char ch)
	{
		return ch >= 'A' && ch <= 'Z';
	}

	private static boolean isLower(char ch)
	{
		return ch >= 'a' && ch <= 'z';
	}
